#include <stdlib.h>
#include <stdio.h>
#include <iostream>
#include <string>
#include <list>
#include <map>

class City{
public:
	std::string name;
	int vertIndex;
	int toll;
};
class EdgeNode{
public:
	EdgeNode* nextNode;
};

class Map{

public:
	std::map<std::string,City> map;
	std::list<City> list;
	int numCities;
};




